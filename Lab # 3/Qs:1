#include <iostream>
using namespace std;
class Student {
public:
    string name;
    int rollNumber;
};
int main() {
    Student s1;
    s1.name = "Syed Muhammad Furqan";
    s1.rollNumber = 766;
    cout << "Student Name : " << s1.name << endl;
    cout << "Roll Number : " << s1.rollNumber << endl;
    return 0;
}
