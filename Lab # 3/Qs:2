#include <iostream>
using namespace std;
class Student {
private:
    int rollNumber;  
public:
	string name;
    void setRollNo(int r) {   
        rollNumber = r;
    }
    int getRollNo() {     
        return rollNumber;
    }
};
int main() {
    Student s1;
    s1.name = "Syed Muhammad Furqan";
    s1.setRollNo(766); 
    cout << "Student Name : " << s1.name << endl;
    cout << "Roll Number : " << s1.getRollNo() << endl;
    return 0;
}
